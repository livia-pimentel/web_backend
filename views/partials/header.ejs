<header class="top-header" id="top-header">
    <span class="siteName">
        <img src="/images/site/icon.png" alt="Logo Image">
        <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <div class="tools" id="tools">
        <%# console.log("Now I can See: ", locals) %>
        <% if (locals.accountData) { %>
            <a href="/account/">Welcome <%= locals.accountData.account_firstname %></a>
